<script src="https://static.tumblr.com/0podkko/jfMplm3f8/modified_photosets.js"></script>
<script src="https://static.tumblr.com/0podkko/qWqq8va08/photosets.js"></script>
<link href="https://static.tumblr.com/0podkko/xIbplezmo/modified_photosets.css" rel="stylesheet">
<!--video resizing-->
<script src="https://rawgit.com/robinpx/tumblr/master/scripts/flexibleFrames/flexibleFrames.min.js"></script>
<!--smooth scroll-->
<script src="https://cdnjs.cloudflare.com/ajax/libs/smoothscroll/1.4.10/SmoothScroll.min.js"></script>
<!--feather icons-->
<script src="https://unpkg.com/feather-icons@4.9.0/dist/feather.min.js"></script>
<!--scrolling animation-->
<link href="https://cdn.rawgit.com/michalsnik/aos/2.1.1/dist/aos.css" rel="stylesheet">
<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
<!--tooltips-->
<script type="text/javascript" src="https://static.tumblr.com/cqvi50o/VSzox2ncv/style-my-tooltips.js"></script>

<script>
//icons
feather.replace();

//scroll
SmoothScroll({
animationTime:600 
});

//fade
$(function() {
$('body').removeClass('fade-out');
});

// tooltips
$("[title]").style_my_tooltips({
tip_follows_cursor: false,
tip_delay_time: 100,
tip_fade_speed: 300
});

//photosets
undoPhotoset({
'posts': '.text-post',
'text class': '',
'photoset class': ''
},false);
   
initPhotosets();

//flexible frames
function flexFrame() {
$(".caption").each(function() {
$(this).find("iframe").wrap("<div class='capframe'></div>");
flexibleFrames($(".capframe"));
});
flexibleFrames($(".video"));
}
$(document).ready(flexFrame);

//remove redirects
$('a[href*="t.umblr.com/redirect"]').each(function(){
var originalURL = $(this).attr("href").split("?z=")[1].split("&t=")[0];
var replaceURL = decodeURIComponent(originalURL);
$(this).attr("href", replaceURL);
});

{block:iffadingposts}    
// aos
AOS.init({offset: 25,duration: 1000});
{/block:iffadingposts}

//audio player
$('body').tooltip({track:false});    
$('iframe.tumblr_audio_player').load( function() {
$('iframe.tumblr_audio_player').contents().find("head")
.append($("<style type='text/css'> .audio-player{background:{color:lighter accent} !important} </style>"));
});

// scroll to top
$(window).scroll(function(){
if ($(this).scrollTop() > 100) {
$('.scrollup').fadeIn();
} else {
$('.scrollup').fadeOut();
}
});
$('.scrollup').click(function(){
$("html, body").animate({ scrollTop: 0 }, 1000);
return false;
});

//notes shortening
var $container = $('.posts');
$container.find('.notecount').each(function(){
var n = $(this).html().split(' ')[0].replace(/,/g, '');
if (n > 999) {
n = Math.floor(n / 100) / 10;
$(this).text(n + 'k');
}
});

</script>

