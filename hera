feather.replace();

SmoothScroll({
animationTime:600 
});

$(function() {
$('body').removeClass('fade-out');
});
    
// tooltips
$("[title]").style_my_tooltips({
tip_follows_cursor: false,
tip_delay_time: 100,
tip_fade_speed: 300
});

